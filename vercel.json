// KM/vercel.json
// https://github.com/KrystianMaccs/KM.git

{
    "builds": [{
        "src": "MySite/wsgi.py",
        "use": "@ardnt/vercel-python-wsgi",
        "config": { "maxLambdaSize": "15mb" }
    }],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "MySite/wsgi.py"
        }
    ]
}